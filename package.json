{
  "name": "join-table",
  "description": "Collection class for many-to-many joins.",
  "version": "2.0.0",
  "ava": {
    "files": "dist/**/*.test.js",
    "babel": {},
    "source": [
      "dist/**/*.js"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 7.8
          }
        }
      ]
    ],
    "plugins": [
      "transform-flow-strip-types"
    ]
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "del-cli": "^0.2.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb-base": "^11.1.2",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "^0.42.0"
  },
  "engines": {
    "node": ">=7.8"
  },
  "files": [
    "dist"
  ],
  "main": "dist/join-table.js",
  "license": "MIT",
  "repository": "callumlocke/join-table",
  "scripts": {
    "clean": "del dist/**",
    "compile": "babel src --out-dir dist --source-maps --copy-files",
    "types": "flow gen-flow-files src/join-table.js --out-dir dist"
  }
}
