{
  "name": "join-table",
  "description": "Collection class for many-to-many joins.",
  "version": "1.2.3",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "transform-flow-strip-types"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.11.6"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "del-cli": "^0.2.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "flow-bin": "^0.32.0",
    "mocha": "^3.0.2"
  },
  "engines": {
    "node": ">=0.12"
  },
  "eslintConfig": {
    "extends": "airbnb/base",
    "root": "true",
    "parser": "babel-eslint",
    "rules": {
      "no-underscore-dangle": 0
    }
  },
  "files": [
    "dist/lib",
    "src/lib"
  ],
  "jsnext:main": "src/lib/index.js",
  "license": "MIT",
  "main": "dist/lib/index.js",
  "repository": "callumlocke/join-table",
  "scripts": {
    "build": "del dist && babel src --out-dir dist --source-maps",
    "build:watch": "del dist && babel src --out-dir dist --source-maps --watch",
    "prepublish": "npm run build",
    "test": "eslint . && npm run build && cd dist && mocha",
    "test:watch": "cd dist && mocha --watch"
  }
}
