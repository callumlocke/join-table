{
  "name": "join-table",
  "description": "Collection class for many-to-many joins.",
  "version": "1.2.2",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "transform-flow-strip-types"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "del-cli": "^0.2.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-babel": "^3.2.0",
    "mocha": "^2.4.5"
  },
  "engines": {
    "node": ">=0.12"
  },
  "eslintConfig": {
    "extends": "airbnb/base",
    "root": "true",
    "parser": "babel-eslint"
  },
  "files": [
    "dist"
  ],
  "jsnext:main": "src/lib/index.js",
  "license": "MIT",
  "main": "dist/lib/index.js",
  "repository": "callumlocke/join-table",
  "scripts": {
    "build": "del dist && babel src --out-dir dist --source-maps",
    "build:watch": "del dist && babel src --out-dir dist --source-maps --watch",
    "prepublish": "npm run build",
    "test": "eslint . && npm run build && cd dist && mocha",
    "test:watch": "cd dist && mocha --watch"
  }
}
